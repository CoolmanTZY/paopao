.home-page{
  padding-top: 100rpx;
}
/* 搜素框 */
.search{
  padding:20rpx;
  width: 750rpx;
  background: #ff2d4a;
  position: fixed;
  top:0rpx;
  left: 0;
  z-index: 5;
}
.search .search-input{
  height: 50rpx;
  width: 680rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-radius: 3px;
  background: #fff;
  color: black;
}

/* 轮播图 */
.catitems{
  padding: 20rpx;
  display: flex;
  width: 100%;
  justify-content: space-between;
  box-sizing: border-box;
}
.catitems image{
  width:128rpx;
  height: 140rpx;
}

/* 楼层(内容) */
.floordata{
  width: 100%;
}
.floor-title image{
  width: 100%;
  height: 60rpx;
}
.floor-content{
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}
.floor-content .floor-cnt-left image{
  width: 232rpx;
  height: 386rpx;
}
.floor-cnt-right{
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-left: 10rpx;
}
.floor-content .floor-cnt-right image{
  width: 232rpx;
  height: 188rpx;
}