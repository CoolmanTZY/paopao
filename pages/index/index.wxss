.home-page{
  padding-top: 100rpx;
}
.swiper{
  padding: 0rpx 35rpx 10rpx; 
}

/* 导航栏 */
.catitems{
  padding:0 20rpx 10rpx;
  display: flex;
  width: 100%;
  justify-content: space-between;
  box-sizing: border-box;
}
.catitems image{
  width:128rpx;
  height: 140rpx;
}

/* 楼层(内容) */
.floordata{
  width: 100%;
}
.floor-title image{
  width: 100%;
  height: 60rpx;
}
.floor-content{
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}
.floor-content .floor-cnt-left image{
  width: 232rpx;
  height: 386rpx;
}
.floor-cnt-right{
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-left: 10rpx;
}
.floor-content .floor-cnt-right image{
  width: 232rpx;
  height: 188rpx;
}